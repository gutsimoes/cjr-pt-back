generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model user {
  id          Int          @id @default(autoincrement())
  nome        String
  email       String       @unique
  senha       String
  departamento String
  curso       String
  fotoPerfil Bytes?      
  createdAt   DateTime     @default(now())
  updatedAt   DateTime     @updatedAt
}

